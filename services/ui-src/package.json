{
  "name": "ui-src",
  "description": "",
  "version": "1.0.0",
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@cmsgov/design-system": "^3.3.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.8",
    "aws-amplify": "^4.3.20",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "framer-motion": "^6",
    "object-path": "^0.11.8",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^12.1.0",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-idle-timer": "^5.0.0",
    "react-query": "^3.38.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "react-textarea-autosize": "^8.3.3",
    "sass": "^1.51.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "coverage": "jest --coverage",
    "test": "jest --setupFiles dotenv/config",
    "watchTest": "jest --setupFiles dotenv/config --watch",
    "eject": "react-scripts eject",
    "postinstall": "patch-package"
  },
  "jest": {
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "src/views/**/*.{ts,tsx}",
      "!src/components/CoreQuestions/index.tsx",
      "!src/components/ScrollToTop/index.tsx",
      "!src/components/CoreQuestionWrapper/index.tsx",
      "!src/components/index.tsx",
      "!src/components/LocalLogins/index.tsx",
      "!src/components/LocalLogins/mockUsers.ts",
      "!src/components/Inputs/index.tsx",
      "!src/components/Table/types.ts",
      "!src/components/Layouts/index.tsx",
      "!src/views/NewAmendment/index.tsx",
      "!src/views/Amendments/index.tsx",
      "!src/views/DemoComponents/index.tsx",
      "!src/views/Profile/index.tsx",
      "!src/views/Login/index.tsx",
      "!src/views/AddChildCoreSet/index.tsx",
      "!src/views/AddHHCoreSet/index.tsx",
      "!src/views/AdminHome/index.tsx",
      "!src/views/StateHome/actions.ts",
      "!src/components/Table/_coreSetData.ts",
      "!src/components/Table/_measureData.ts",
      "!src/views/DemoMeasure/**/*.{ts,tsx}",
      "!src/views/ApiTester/index.tsx",
      "!src/components/Upload/index.tsx",
      "!src/views/Qualifiers/**/*.{ts,tsx}",
      "!src/measures/**/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "globals": {
      "_env_": {}
    },
    "testEnvironment": "jest-environment-jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.78.0",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@types/object-path": "^0.11.1",
    "@types/react": "^18.0.8",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "babel": "^6.23.0",
    "babel-jest": "^28.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.2",
    "serverless-cloudfront-invalidate": "^1.11.0",
    "serverless-s3-bucket-helper": "CMSgov/serverless-s3-bucket-helper#0.1.1",
    "serverless-s3-sync": "^3.0.0",
    "typescript": "^4.6.3"
  }
}
